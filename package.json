{
    "name": "chest.store-website",
    "version": "0.0.1",
    "scripts": {
        "build": "npm run clean && npm run build:pug && npm run build:scss && npm run build:scripts && npm run build:assets",
        "build:assets": "node scripts/build-assets.js",
        "build:pug": "node scripts/build-pug.js",
        "build:scripts": "node scripts/build-scripts.js",
        "build:scss": "node scripts/build-scss.js",
        "clean": "node scripts/clean.js",
        "start": "npm run build && node scripts/start.js",
        "start:debug": "npm run build && node scripts/start-debug.js"
    },
    "description": "chest.store's website bootstrapped from https://github.com/BlackrockDigital/startbootstrap-grayscale.",
    "homepage": "https://chest.store",
    "bugs": {
        "url": "https://github.com/cheststore/website/issues",
        "email": "support@chest.store"
    },
    "license": "MIT",
    "author": "Lance Whatley",
    "repository": {
        "type": "git",
        "url": "https://github.com/cheststore/website.git"
    },
    "dependencies": {
        "bootstrap": "4.5.0",
        "highlight.js": "^10.1.2",
        "toastr": "^2.1.4"
    },
    "devDependencies": {
        "autoprefixer": "9.8.0",
        "browser-sync": "2.26.7",
        "chokidar": "3.4.0",
        "concurrently": "5.2.0",
        "postcss": "7.0.30",
        "prettier": "2.0.5",
        "pug": "2.0.4",
        "sass": "1.26.5",
        "shelljs": "0.8.4",
        "upath": "1.2.0"
    }
}
